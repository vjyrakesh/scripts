echo ga.bat             == git add .
echo gb.bat             == git branch
echo gc.bat [message]   == git commit -m [message]
echo gcb.bat [branch]   == git checkout [branch]
echo gcm.bat            == git checkout master 
echo gcnb.bat [branch]  == git checkout -b [branch]
echo gm.bat [branch]    == git merge [branch]
echo gpob.bat [branch]  == git push origin [branch]
echo gpom.bat           == git push origin master
echo gs.bat             == git status
